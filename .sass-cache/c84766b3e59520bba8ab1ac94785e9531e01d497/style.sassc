  &.ng-enter, &.ng-leave
    transition: all 500ms ease
    transition-delay: 250ms
  
  &.slideLeft
    &.ng-enter
      transform: translateX(100%)
    &.ng-enter-active
      transform: translateX(0%)

    &.ng-leave
      transform: translateX(0%)
    &.ng-leave-active
      transform: translateX(-100%)


  &.slideRight
    &.ng-enter
      transform: translateX(-100%)
    &.ng-enter-active
      transform: translateX(0%)

    &.ng-leave
      transform: translateX(0%)
    &.ng-leave-active 
      transform: translateX(100%)


