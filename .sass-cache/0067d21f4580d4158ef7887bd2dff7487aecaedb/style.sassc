  overflow: hidden

%nav-elements
  position: fixed
  text-align: center
  left: 50%
  margin-left: -2.5em
  cursor: pointer
  // z-index: 100

  background: rgba(0,0,0,0.3)
  fill: rgba(255,255,255,1.0)

  transition: background 300ms, fill 300ms

  .no-touch &:hover
    background: rgba(0,0,0,0.6)
    fill:   rgba(255,255,255,1.0)
    cursor: pointer

