    display: none

  .hot-spot-toggle
    cursor: pointer
    height: 30px
    position: absolute
    right: 10px
    top: 0
    width: 30px
    .icon
      height: 30px
      width: 30px
    use
      fill: $light
    .icon-minus
      display: none


// Hot Spot
// ~~~~~~~~

.modal-outer
  bottom: 0
  background: $dark
  color: $light
  left: 0
  overflow: auto
  position: absolute
  top: 0
  width: 100%

.modal-close
  cursor: pointer
  position: absolute
  right: 10px
  top: 0px
