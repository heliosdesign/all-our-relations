          text-align: left
  
    img
      width: 100%
      height: auto

      &.tall
        width: auto
        height: 30%
        margin: 0 auto
        position: relative
    
    p.mediumtext
      text-align: left
      font-size: 1.2em
      line-height: 1.4em

      strong
        color: #E68A00
    
      &.pullquote
        font-size: 1.5em
