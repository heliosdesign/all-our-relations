      height: 0

      iframe 
        position: absolute
        top: 0
        left: 0
        width: 100%
        height: 100%
    
    h1
      width: 100%
      text-align: center
      text-transform: capitalize
      font-weight: normal
      padding-top: 0.25em
      padding-bottom: 0.25em
