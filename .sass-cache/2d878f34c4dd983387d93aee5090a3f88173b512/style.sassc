      color: #E68A00
      .portrait
        opacity: 0.7

    .portrait
      height: ($paragraph-width/6) - ($paragraph-width*.02)
      width: ($paragraph-width/6) - ($paragraph-width*.02)
      position: relative
      background-size: cover
      border-radius: ($paragraph-width/3) - ($paragraph-width*.01)
      margin-bottom: 1.5em
      border: 1px solid #555555
      
