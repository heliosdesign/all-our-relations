    .header
      display: table
      width: 100%
      margin: 0px
      height: 500px
      background-size: cover
      background-position: center calc(center - 50px)

  .content
    top: 150px
    padding: 0em 1em 0px 1em
    background: rgba(255,255,255,.6)

    p
      font-size: 0.8em
      line-height: 1.45em
