      padding-top: 0.25em
      padding-bottom: 0.25em
      // margin-top: 1em
      margin-bottom: 1em
      border-bottom: 2px solid
      border-top: 2px solid
      .secondary
        font-size: 0.75em

    ol
      margin-bottom: 6em
      li
        margin-bottom: 0.75em
        ol
          padding-top: 1em
