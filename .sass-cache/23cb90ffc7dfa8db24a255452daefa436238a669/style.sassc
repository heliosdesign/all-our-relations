// Globals
// ~~~~~~~

$red: #ed1b2e
$dark:  #222222
$light: #fff

$paragraph-width: 800px

@mixin breakpoint($point)
  // < 550px or 35em.
  @if $point == small
    @media (max-width: 600px) 
      @content

  // > 550px or 35em.
  @if $point == medium
    @media (min-width: 600px) 
      @content

  // > 850px or 53.123em.
  @if $point == large
    @media (min-width: 850px) 
      @content

  // > 1000px or 64em.
  @if $point == full
    @media (min-width: 1000px) 
      @content

%ir
  text-indent: 101%
  white-space: nowrap
  overflow: hidden

%nav-elements
  position: fixed
  text-align: center
  left: 50%
