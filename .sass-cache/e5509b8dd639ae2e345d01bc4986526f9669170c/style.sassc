  // Prev/Next Day
  // ~~~~~~~~~~~~~

  a.dayNavLink
    display: block
    position: fixed
    cursor: pointer

    color: $light
    border-color: $light
    //background-color: rgba(0,0,0,0.2)
    height: 80px
    width: 74px
    z-index: 200


    &:hover use
      fill: $red !important
      stroke: $red !important

