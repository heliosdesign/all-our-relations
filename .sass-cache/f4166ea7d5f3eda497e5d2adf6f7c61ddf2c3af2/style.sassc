.ir
  text-indent: 101%
  white-space: nowrap
  overflow: hidden

.cf:after
  visibility: hidden
  display: block
  font-size: 0
  content: " "
  clear: both
  height: 0

*
  html .clearfix, &:first-child + html .clearfix
    zoom: 1


#background
  width: 100%
  height: 100%

// /* IE6
// /* IE7

@mixin breakpoint($point)
  // < 550px or 35em.


  @if $point == reallyshort
    @media (max-height: 400px)
      @content

  @if $point == small
    @media (max-width: 598px),(max-height: 500px)
      @content
  

  // > 550px or 35em.
  @if $point == medium
    @media (min-width: 599px) and (max-width : 899px) and (min-height: 501px) and (max-height: 650px)
      @content

  // > 550px or 35em.
  @if $point == medheight
    @media (min-width: 600px) and (max-height: 650px)
      @content

  // > 850px or 53.123em.
  @if $point == large
    @media (min-width: 900px) and (min-height: 651px)
      @content

  // > 1000px or 64em.
  @if $point == full
    @media (min-width: 1000px) 
      @content
        

body
  background: #000000
  overflow: hidden
  font: 'Domine', serif

canvas
  position: absolute
  width: 100%
  top: 0px
  left: 0px

svg
  position: absolute
  width: 100%
  height: 100%
  top: 60px
  left: 120px
  // display: none
  // /*filter:url(#dropshadow)

// /*D3 Styles
#svg-container
  position: absolute
  width: 100%
  height: 100%
  top: 0px
  left: 0px

#all-container
  position: absolute
  width: 100%
  height: 100%
  top: 0px
  left: 0px
  
  .portraitwrapper,.resourcewrapper,.showwrapper
    position: absolute
    top: 0px
    left: 20px
    height: 100%
    display: table
    .side-menu
      vertical-align: middle
      display: none
      .portrait
        width: 60px
        height: 60px
        border: solid 4px
        border-color: #FFF
        border-radius: 100%
        background-size: cover
        filter: sepia(0%)
        cursor: pointer
        margin-bottom: 2vh
        box-shadow: 3px 4px 3px rgba(0,0,0,0.5)
        transition: all 500ms
        &.active
          border-color: #291F00
          filter: sepia(100%)
  .resourcewrapper,.showwrapper
    .side-menu
      padding-top: 8em
      vertical-align: top
      display: none
      .resource-btn
        margin-bottom: 1em
        padding: 0.5em
        cursor: pointer
        width: 180px
        text-align: center
        color: black
        transition: all .5s
        text-transform: uppercase
        &.btn-active
          color: #b07e1b 
  
  .background-shifter
    transition: all 2s

.resourcecontainer,.showscontainer
  height: calc(100% - 90px)
  width: 60%
  max-width: 650px
  position: relative
  margin: 0px auto
  top: 75px
  left: 0px
  display: none
  background: rgba(255, 255, 255, 0.5)
  padding: 1em
  padding-bottom: 0px
  overflow-y: scroll

  #step1,#step2,#step3,#step4,.steps,#show1,#show2,#show3,#show4,.shows
    position: relative
    height: 100%
    width: 100%
    display: none

    a
      text-decoration: none
      color: #b07e1b



    .videoWrapper 
      position: relative
      padding-bottom: 56.25% /* 16:9 */
      padding-top: 25px
      height: 0
      margin-bottom: 1em
      margin-top: 1em

      iframe 
        position: absolute
        top: 0
        left: 0
        width: 100%
        height: 100%
    
    h1
      width: 100%
      text-align: center
      text-transform: capitalize
      font-weight: normal
      padding-top: 0.25em
      padding-bottom: 0.25em
      // margin-top: 1em
      margin-bottom: 1em
      border-bottom: 2px solid
      border-top: 2px solid

    ol
      margin-bottom: 6em
      li
        margin-bottom: 0.75em
        ol
          padding-top: 1em
          li
            margin-bottom: 0.75em
            

  // #step1
  //   display: block

#milestones
