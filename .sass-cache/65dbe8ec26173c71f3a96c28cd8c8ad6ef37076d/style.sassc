.ir
  text-indent: 101%
  white-space: nowrap
  overflow: hidden

.cf:after
  visibility: hidden
  display: block
  font-size: 0
  content: " "
  clear: both
  height: 0

*
  html .clearfix, &:first-child + html .clearfix
    zoom: 1


#background
  width: 100%
  height: 100%

// /* IE6
// /* IE7

@mixin breakpoint($point)
  // < 550px or 35em.


  @if $point == reallyshort
    @media (max-height: 400px)
      @content

  @if $point == small
    @media (max-width: 598px),(max-height: 500px)
      @content
  

  // > 550px or 35em.
  @if $point == medium
    @media (min-width: 599px) and (max-width : 899px) and (min-height: 501px) and (max-height: 650px)
      @content

  // > 550px or 35em.
  @if $point == medheight
    @media (min-width: 600px) and (max-height: 650px)
      @content

  // > 850px or 53.123em.
  @if $point == large
    @media (min-width: 900px) and (min-height: 651px)
      @content

  // > 1000px or 64em.
  @if $point == full
    @media (min-width: 1000px) 
      @content
        

body
  background: #000000
  overflow: hidden
  font: 'Domine', serif

canvas
  position: absolute
  width: 100%
  top: 0px
  left: 0px

svg
  position: absolute
  width: 100%
  height: 100%
  top: 60px
  left: 120px
  // display: none
  // /*filter:url(#dropshadow)

// /*D3 Styles
#svg-container
  position: absolute
  width: 100%
  height: 100%
  top: 0px
  left: 0px

#all-container
  position: absolute
  width: 100%
  height: 100%
  top: 0px
  left: 0px
  
  .portraitwrapper,.resourcewrapper,.showwrapper
    position: absolute
    top: 0px
    left: 20px
    height: 100%
    display: table
    .side-menu
      vertical-align: middle
      display: none
      .portrait
        width: 60px
        height: 60px
        border: solid 4px
        border-color: #FFF
        border-radius: 100%
        background-size: cover
        filter: sepia(0%)
        cursor: pointer
        margin-bottom: 2vh
        box-shadow: 3px 4px 3px rgba(0,0,0,0.5)
        transition: all 500ms
        &.active
          border-color: #291F00
          filter: sepia(100%)
  .resourcewrapper,.showwrapper
    .side-menu
      padding-top: 8em
      vertical-align: top
      display: none
      .resource-btn,.subsection
        margin-bottom: 1em
        padding: 0.5em
        cursor: pointer
        width: 180px
        text-align: center
        color: black
        transition: all .5s
        text-transform: uppercase
        &.btn-active
          color: #b07e1b 

      .subsection
        margin-top: 60px
        font-weight: bold
        font-size: 1.5em
        // border-bottom: 1px solid
  
  .background-shifter
    transition: all 2s

