    .chptDeco
      width: 20%
      height: auto
      margin-bottom: 10px

    video
      margin-bottom: 5%

    .paragraph-width
      // position: absolute
      // top: 0px
      // border: 1px solid #000
      // border-width: 1px 0px 1px 0px
      width: 40%
      // padding: 15px
      margin: 3% 30% 0em 30%
      @include breakpoint(small)
        width: 90%
        margin: 3% 5% 0em 5%

      h1
        margin: 0px
        text-transform: uppercase
        font-weight: normal
        font-size: 1.6em
        line-height: 1.8em
        margin-bottom: 1.8em
        letter-spacing: 5px
        border: 1px solid #000
        border-width: 1px 0px 1px 0px

    .title
      width: auto
      height: 100%

    .column
      position: relative
      float: left
      width: 50%
      text-align: left
      h1
        margin-bottom: 10px
      h2
        font-weight: normal
        font-size: 1.1em
        color: #555555
        margin-bottom: 2em

    img
      position: absolute
      top: 2em
      left: 0px
      height: auto
      width: 100%
      margin-bottom: 4em
      
    .pan-down
      transform: translateY(-50%)

  img
    width: 100%
    height: auto
    margin: 0 auto
    margin-bottom: 1em

h1, h2, h3, h4, h5 h6, p
  position: relative
  max-width: $paragraph-width
  margin: 0 auto
  margin-bottom: 1em


.bg
  width: 100%
  max-width: $paragraph-width
  padding: 1em
  margin: 0 auto
  background: rgba(0,0,0,0.35)
  color: $light  

.bg-pos
  width: 100%
  height: 100%
  background: rgba(0,0,0,0.2)

.pos-title

  h1,h2,h3
    margin: 0

  .uppercase
    text-transform: uppercase
