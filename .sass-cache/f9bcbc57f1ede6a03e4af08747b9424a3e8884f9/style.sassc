  font-size: 1.5em
  display: none 

  &.timeline
    .content
      position: absolute
      padding: 0em 1em 2em 1em

  &.intro
    opacity: 0
    margin-top: 10%
    // margin-top: 150px

    .content
      padding: 0em 1em 1em 1em
      height: 100%
      top: 0px !important

      p
        font-size: 0.8em
        line-height: 1.45em
        a
          color: #b07e1b
          text-decoration: none
          &:hover
            color: grey
      ul
        font-size: 0.8em
        padding-left: 0px
        li
          list-style: none

  &.active
    opacity: 1
    transition: all 1000ms

  .exitbtn
    cursor: pointer
    position: absolute
    top: 0px
    right: 0px
